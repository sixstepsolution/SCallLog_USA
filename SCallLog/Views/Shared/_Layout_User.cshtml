<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>User | Dashboard</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.6 -->
    <link rel="stylesheet" href="~/Assets/SCall/bootstrap/css/bootstrap.min.css">
    <!-- Font Awesome -->
    @*<link href="~/Assets/SCall/bootstrap/css/font-awesome.min.css" rel="stylesheet" />*@
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    @*<link href="~/Assets/SCall/bootstrap/css/ionicons.min.css" rel="stylesheet" />*@
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <link rel="stylesheet" href="~/Assets/SCall/plugins/datatables/dataTables.bootstrap.css">
    <!-- jvectormap -->
    <link rel="stylesheet" href="~/Assets/SCall/plugins/jvectormap/jquery-jvectormap-1.2.2.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Assets/SCall/dist/css/AdminLTE.min.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="~/Assets/SCall/dist/css/skins/_all-skins.min.css">

    <!-- jQuery 2.2.3 -->
    <script src="~/Assets/SCall/plugins/jQuery/jquery-2.2.3.min.js"></script>

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->


    <script src="../Scripts/angular.min.js"></script>
    @*<script src="~/Scripts/angular.js"></script>*@
    <link href="~/Content/alertifyjs/themes/default.css" rel="stylesheet" />
    <link href="~/Content/alertifyjs/alertify.css" rel="stylesheet" />
    <script src="~/Content/alertifyjs/alertify.min.js"></script>
    <script src="~/Scripts/angular-route.min.js"></script>
    <script src="~/Scripts/ngStorage.min.js"></script>
    <script src="http://angular-ui.github.io/bootstrap/ui-bootstrap-tpls-0.11.0.js"></script>

    <script src="~/Scripts/angular-sanitize.js"></script>
    <script src="~/AngularJS/scall.js"></script>
    <style>
        .skin-blue .main-header .navbar {
            background-color: #0d0326 !important;
        }

        .skin-blue .main-header .logo {
            background-color: #0d0326 !important;
            color: #fff;
            border-bottom: 0 solid transparent;
        }

        .skin-blue .main-header .navbar .sidebar-toggle {
            color: #fff;
        }

            .skin-blue .main-header .navbar .sidebar-toggle:hover {
                background-color: #0d0326 !important;
                color: #fff;
            }

        .sidebar {
            background-color: #e3e3e3 !important;
            padding-bottom: 10px;
            color: #000 !important
        }

        .skin-blue .wrapper, .skin-blue .main-sidebar, .skin-blue .left-side {
            background-color: #e3e3e3 !important;
            color: #000 !important
        }

        .skin-blue .sidebar-menu > li:hover > a, .skin-blue .sidebar-menu > li.active > a {
            color: #000 !important;
            background: #e3e3e3 !important;
            border-left-color: #35379a !important;
        }

        .skin-blue .sidebar-menu > li.header {
            color: #000 !important;
            background: #ff513b !important;
        }

        .skin-blue .main-header li.user-header {
            background-color: #0d0326 !important;
            color: #000 !important;
        }

        .skin-blue .sidebar a {
            color: #000000 !important;
        }

        .btn-custom {
            color: #fff;
            background-color: #d54a4f !important;
            border-color: #d54a4f !important;
        }

            .btn-custom:hover {
                color: #fff;
                background-color: #0d0326 !important;
                border-color: #0d0326 !important;
            }

        .btn-success {
            color: #fff;
            background-color: #0d0326 !important;
            border-color: #0d0326 !important;
        }

            .btn-success:hover {
                color: #fff;
                background-color: #d54a4f !important;
                border-color: #d54a4f !important;
            }

        .skin-blue .sidebar .treeview-menu a {
            color: #fff !important;
        }
    </style>
</head>
<body ng-app="SCALL" class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="../SCall/Index" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>SCALL</b></span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>SCALL</b></span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <!-- Navbar Right Menu -->
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">

                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="~/Attachments/@Session["SCL_user_profile"]" class="user-image" alt="User Image">
                                <span class="hidden-xs">@Session["username_SCL_account"]</span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header">
                                    <img src="~/Attachments/@Session["SCL_user_profile"]" class="img-circle" alt="User Image">
                                    <p>
                                        @Session["name_SCL_account"]
                                        <small>@Session["email_SCL_account"]</small>
                                    </p>
                                </li>
                                <!-- Menu Body -->
                                <!-- Menu Footer-->
                                <li class="user-footer">
                                    <div class="pull-left">
                                        <a href="#" class="btn btn-default btn-flat">Profile</a>
                                    </div>
                                    <div class="pull-right">
                                        <a href="../Master/Logout" class="btn btn-default btn-flat">Sign out</a>
                                    </div>
                                </li>
                            </ul>
                        </li>
                        <!-- Control Sidebar Toggle Button -->
                        <li>
                            <a href="#" data-toggle="control-sidebar"><i class="fa fa-gears"></i></a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="image">
                        <img src="~/Attachments/@Session["SCL_company_picture"]" alt="User Image">
                    </div>
                    <h2 style="color:white">@Session["name_SCL_account"]</h2>
                </div>

                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu">
                    <li class="header">MAIN NAVIGATION</li>
                    <li id="active_dash"><a href="../User"><i class="fa fa-dashboard"></i> <span>DashBoard</span></a></li>

                    <li class="treeview" id="active_jobs">
                        <a href="#">
                            <i class="fa fa-files-o"></i>
                            <span>Job Cards</span>
                            <span class="pull-right-container">
                                <span class="label label-primary pull-right"><i class="fa fa-angle-left"></i></span>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li id="active_all_jobs"><a href="../User/AllocatedComplaints"><i class="fa fa-file-archive-o text-aqua"></i>Assigned JobCards</a></li>
                            <li id="active_view_jobs"><a href="../User/ViewJobCards"><i class="fa fa-file-archive-o text-aqua"></i>View JobCards</a></li>
                            <li id="active_map_jobs"><a href="../User/AsignedComplaintsMap"><i class="fa fa-file-archive-o text-aqua"></i>View Map</a></li>
                        </ul>
                    </li>
                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->


        <div class="wrapper">
            @RenderBody()
        </div>

        <footer class="main-footer">

            <strong>Copyright &copy; 2021 <a href="">SCALL</a>.</strong> All rights
            reserved.
        </footer>

        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Create the tabs -->
            <ul class="nav nav-tabs nav-justified control-sidebar-tabs"></ul>
            <!-- Tab panes -->
            <div class="tab-content">
                <!-- Home tab content -->
                <div class="tab-pane" id="control-sidebar-home-tab">
                </div>
                <!-- /.tab-pane -->


            </div>
        </aside>
        <!-- /.control-sidebar -->
        <!-- Add the sidebar's background. This div must be placed
             immediately after the control sidebar -->
        <div class="control-sidebar-bg"></div>

    </div>
    <!-- ./wrapper -->
    <!-- Bootstrap 3.3.6 -->
    <script src="~/Assets/SCall/bootstrap/js/bootstrap.min.js"></script>
    @*<script src="~/Assets/SCall/plugins/select2/select2.full.min.js"></script>*@
    <!-- DataTables -->
    <script src="~/Assets/SCall/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="~/Assets/SCall/plugins/datatables/dataTables.bootstrap.min.js"></script>
    <!-- SlimScroll 1.3.0 -->
    <script src="~/Assets/SCall/plugins/slimScroll/jquery.slimscroll.min.js"></script>

    <!-- FastClick -->
    <script src="~/Assets/SCall/plugins/fastclick/fastclick.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Assets/SCall/dist/js/app.min.js"></script>
    <!-- Sparkline -->
    <script src="~/Assets/SCall/plugins/sparkline/jquery.sparkline.min.js"></script>
    <!-- jvectormap -->
    <script src="~/Assets/SCall/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="~/Assets/SCall/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <!-- ChartJS 1.0.1 -->
    <script src="~/Assets/SCall/plugins/chartjs/Chart.min.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="~/Assets/SCall/dist/js/demo.js"></script>

</body>
</html>