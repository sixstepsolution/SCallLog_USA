                                <p class="text-center">
                                    <strong>Complaints: 1 Jan, {{year}} - 31 Dec, {{year}}</strong>
                                </p>
                                <div class="chart">
                                    <!-- Sales Chart Canvas -->
                                    <canvas id="ComplaintByMonthChart" style="height: 180px;"></canvas>
                                </div>
                                <!-- /.chart-responsive -->
                            </div>
