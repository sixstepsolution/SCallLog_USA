
@{
    ViewBag.Title = "MainIndex";
    Layout = "~/Views/Shared/_Layout_SCL.cshtml";
}
<script>
    $(document).ready(function () {
        $('#active_dash').addClass("active selected");

    });
</script>
<script src="~/AngularJS/main_dashboard.js"></script>
